{
  "name": "@interest-protocol/sui-money-market-sdk",
  "version": "3.2.0",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": "https://github.com/interest-protocol/sui-money-market-sdk",
  "author": "jack poon",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "sdk to interact with interest protocol money market module",
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build:tsup": "tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build:types": "tsc --project tsconfig.json && resolve-tspaths",
    "build": "npm run build:types && npm run build:tsup",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mysten/bcs": "^0.7.2",
    "@mysten/sui.js": "^0.36.0",
    "@pythnetwork/price-service-client": "^1.6.0",
    "bignumber.js": "^9.1.1",
    "ramda": "^0.29.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/jest": "^29.5.0",
    "@types/ramda": "^0.29.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.43.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "resolve-tspaths": "^0.8.13",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
  }
}
